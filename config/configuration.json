{
  "general": {
    "simulated": false,
    "maximum-parallel-jobs": -1,
    "graph": {
      "window-size": 4
    }
  },
  "open_bci": {
    "log_level": "TRACE",
    "board": "SYNTHETIC_BOARD",
    "data-callback-frequency-ms": 50,
    "communication": {
      "serial_port": "/dev/ttyUSB0"
    }
  },
  "preprocessing": [
    {
      "type": "DOWNSAMPLE",
      "parameters": {
        "type": "MEDIAN",
        "period": 100
      }
    },

    {
      "type": "DENOISE",
      "parameters": {
        "type": "MEDIAN",
        "period": 100
      }
    }
  ]
}