{
  "nodes": {
    "root": {
      "open_bci": {
        "module": "models.node.generator",
        "type": "OpenBCIBoard",
        "log_level": "TRACE",
        "board": "SYNTHETIC_BOARD",
        "communication": {
          "serial_port": "COM4"
        },
        "buffer_options": {
          "clear_output_buffer_on_generate": false
        },
        "outputs": {
          "eeg": [
            {
              "node": "signal_check",
              "input": "main"
            }
          ],
          "accelerometer": [],
          "timestamp": []
        }
      }
    },
    "common": {
      "signal_check": {
        "module": "models.node.processing",
        "type": "SignalCheck",
        "buffer_options": {
          "clear_output_buffer_on_data_input": false,
          "clear_input_buffer_after_process": false,
          "clear_output_buffer_after_process": false
        },
        "outputs": {
          "main": []
        }
      }
    }
  }
}