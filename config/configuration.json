{
  "general": {
    "simulated": false,
    "maximum-parallel-jobs": -1,
    "graph": {
      "window-size": 4
    }
  },
  "open_bci": {
    "log-level": "TRACE",
    "board": "SYNTHETIC_BOARD",
    "data-callback-frequency-ms": 50,
    "communication": {
      "serial-port": "/dev/ttyUSB0"
    },
    "channel-mapping": {

    }
  },
  "preprocessing": [
    {
      "type":"DENOISE",
      "parameters":{
        "type": "WAVELET",
        "parameters": {
          "wavelet": "haar",
          "decomposition-level": 2
        }
      }
    },
    {
      "type":"DENOISE",
      "parameters":{
        "type": "ENVIRONMENT",
        "parameters": {
          "noise-type": "SIXTY"
        }
      }
    }
  ]
}