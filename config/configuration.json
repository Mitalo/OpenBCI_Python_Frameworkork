{
  "general": {
    "maximum-parallel-jobs": -1,
    "graph": {
      "window-size": 4
    }
  },
  "open_bci": {
    "log_level": "TRACE",
    "board": "CYTON_BOARD",
    "data-callback-frequency-ms": 50,
    "communication": {
      "serial_port": "/dev/ttyUSB0"
    }
  },
  "preprocessing": [
    {
      "type": "DETREND",
      "parameters": {
        "type": "CONSTANT"
      }
    },
    {
      "type": "FILTER",
      "parameters": {
        "type": "BANDPASS",
        "filter": "BUTTERWORTH",
        "order": 2,
        "center_freq": 51,
        "band_width": 100
      }
    }
  ]
}