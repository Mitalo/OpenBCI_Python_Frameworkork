  "trial": {
    "repetitions": 5,
    "randomize": true,
    "sequences": [
      {
        "name": "Rest",
        "code": 0,
        "duration": {
          "mean": 4,
          "standard-deviation": 1,
          "maximum": 6,
          "minimum": 2
        },
        "cue": {
          "file": "console_print.py",
          "parameters": {
            "message": "REST"
          }
        }
      },
      {
        "name": "Close left fist",
        "code": 1,
        "duration": {
          "mean": 2,
          "standard-deviation": 0.5,
          "maximum": 3,
          "minimum": 1
        },
        "cue": {
          "file": "console_print.py",
          "parameters": {
            "message": "CLOSE LEFT FIST"
          }
        }
      },
      {
        "name": "Rest",
        "code": 0,
        "duration": {
          "mean": 4,
          "standard-deviation": 1,
          "maximum": 6,
          "minimum": 2
        },
        "cue": {
          "file": "console_print.py",
          "parameters": {
            "message": "REST"
          }
        }
      },
      {
        "name": "Close right fist",
        "code": 2,
        "duration": {
          "mean": 2,
          "standard-deviation": 0.5,
          "maximum": 3,
          "minimum": 1
        },
        "cue": {
          "file": "console_print.py",
          "parameters": {
            "message": "CLOSE RIGHT FIST"
          }
        }
      }
    ]
  },
  "preprocessing": [
    {
      "type": "CUSTOM",
